{
  "data": {
    "url": "https://raw.githubusercontent.com/dhimmel/biorxiv-licenses/218fd259db928eacf60ad61f6968c3c356beb74c/figure/license-vs-subject/vega-lite-data.json"
  },
  "width": 300,
  "height": 255,
  "transform": {
    "calculate": [{
      "field": "license_index",
      "expr": "datum.License === 'CC BY' ? 0 : datum.License === 'CC BY-ND' ? 1 : datum.License === 'CC BY-NC' ? 2 : datum.License === 'CC BY-NC-ND' ? 3 : 4"
    }]
  },
  "config": {
    "mark": {
      "stacked": "normalize"
    }
  },
  "encoding": {
    "color": {
      "field": "License",
      "scale": {
        "domain": [
          "CC BY",
          "CC BY-ND",
          "CC BY-NC",
          "CC BY-NC-ND",
          "None"
        ],
        "range": [
          "#F68212",
          "#AC5B0C",
          "#934E0A",
          "#492705",
          "#000000"
        ],
        "type": "ordinal"
      },
      "type": "nominal"
    },
    "y": {
      "axis": {
        "axisWidth": 0.0
      },
      "field": "Subject",
      "title": "",
      "type": "nominal"
    },
    "x": {
      "aggregate": "count",
      "axis": {
        "axisWidth": 0.0,
        "grid": false,
        "labels": false,
        "ticks": 0.0
      },
      "field": "*",
      "title": "",
      "type": "quantitative"
    },
    "order": {
      "field": "license_index",
      "type": "quantitative",
      "sort": "descending"
    }
  },
  "mark": "bar"
}